#!/usr/bin/env python3
# -*- coding: utf-8 -*-

import math # импортируем каталог math
import numpy # +numpy
import matplotlib.pyplot as mpp # +что-то?!♥))

# Эта программа рисует график функции, заданной выражением ниже

if __name__=='__main__': # запускаем условный оператор if
    arguments = numpy.arange(0, 200, 0.1) # процедура создания массива
    mpp.plot(
        arguments,
        [math.sin(a) * math.sin(a/20.0) for a in arguments] # что-то похожее на генератор
    )
    mpp.show()
